DEF Test()
; E100 trailing whitespace
INT someVariable
; E201 lower or mixed case built-in type
INT someOtherVariable
REAL someThirdVariable

; E200 lower or mixed case keyword
IF (someVariable) THEN
   ; Do something
ENDIF

; E200 lower or mixed case keyword
foo B_AND bar

; E101 missing whitespace arround operator
